algoritmo "Maior numero"

var
   PRIMEIRO_NUMERO,SEGUNDO_NUMERO:REAL
INICIO

   ESCREVAL("DIGITE OS DOIS NÚMEROS:")
   LEIA(PRIMEIRO_NUMERO,SEGUNDO_NUMERO)
   SE (PRIMEIRO_NUMERO>SEGUNDO_NUMERO) ENTAO
      ESCREVAL(PRIMEIRO_NUMERO)
   SENAO
      SE   (SEGUNDO_NUMERO>PRIMEIRO_NUMERO) ENTAO
         ESCREVAL(SEGUNDO_NUMERO)
      SENAO
         ESCREVAL(PRIMEIRO_NUMERO," = ",SEGUNDO_NUMERO)
      FIMSE
   FIMSE
FIMALGORITMO